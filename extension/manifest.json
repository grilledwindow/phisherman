{
  "manifest_version": 3,
  "name": "Phisherman",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "file:///*", "http://localhost:*/gmail", "http://localhost:*/", "http://127.0.0.1:*/", "http://127.0.0.1:*/gmail"],
      "js": ["index.js"],
      "css": ["index.css"]
    }
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {}
}

